<p id="notice"><%= notice %></p>

<h1>Listing Plasmid Batch Qc Attachments</h1>

<table>
  <thead>
    <tr>
      <th>Attachment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plasmid_batch_qc_attachments.each do |plasmid_batch_qc_attachment| %>
      <tr>
        <td><%= plasmid_batch_qc_attachment.attachment %></td>
        <td><%= link_to 'Show', plasmid_batch_qc_attachment %></td>
        <td><%= link_to 'Edit', edit_plasmid_batch_qc_attachment_path(plasmid_batch_qc_attachment) %></td>
        <td><%= link_to 'Destroy', plasmid_batch_qc_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plasmid batch qc attachment', new_plasmid_batch_qc_attachment_path %>
