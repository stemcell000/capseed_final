<%= render :partial => 'components/process_pills' %>
	<div class="panel panel-primary">
			<div class="panel-heading"> Assay: <%= @assay.name %></div>
			<div class="panel-body">
				
					<div id="clone-list-container">
							<%= render :partial => "plasmid_batches/plasmid_batch_components/list_to_close" , :locals => {:clones => @clones, :assay => @assay}%>
					</div>
					<div class="row">
						<div id= "lock_container" class="col-lg-12">
							<%= render :partial => 'assays/assay_components/complete_lock' %>			
						</div>
					</div>
			<div class="panel-footer row">
				    <div class="btn-group col-md-5" role="group" aria-label="...">
				    	<%= link_to 'Previous', plasmid_batch_assay_path(@assay.id), :class => "btn btn-default btn-sm" %>
						<%= link_to 'Close', complete_assay_path(@assay.id), :class => "btn btn-default btn-sm" %>
				  	</div>
			</div>
		</div>
	</div>
</div>
