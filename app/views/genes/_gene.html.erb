<td ><%= object.id%></td>
<td ><%= object.name%></td>
<td><%= object.created_at.strftime("%b %e, %Y")%></td>
<td class="actions center">
<%if object.clone.nil?%>
	<%= smart_listing_item_actions [
	  	{name: :edit,    url: edit_gene_path(object), title: "Edit"},
		{name: :destroy, url: gene_path(object), title: "Destroy", :confirmation => "Are you sure you want to delete this?"}
		]%>
<%else%>
	<%= smart_listing_item_actions [
	  	{name: :edit,    url: edit_gene_path(object), title: "Edit"},
		]%>
<%end%>
</td>