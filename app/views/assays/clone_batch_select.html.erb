<%= render 'components/process_pills'%>
	<div class="panel panel-primary">
		<div class="panel-heading"> Cloning: <%= @assay.name %></div>
		<div class="panel-body" <%= "data-params-id=#{params[:id]}" if params.has_key?(:id) %> >
				<div id="clone-list-container">
						<%= render :partial => "clone_batches/clone_batch_components/select_list" , :locals => {:clones => @clones, :assay=>@assay}%>
			</div>
			<div class="panel-footer row">
			    <div class="btn-group col-md-5" role="group" aria-label="...">
			    	<%= link_to 'Previous', clone_batch_assay_path(@assay.id), :class => "btn btn-default btn-sm" %>
					<%= link_to 'Next', clone_batch_qc_assay_path(@assay.id), :class => "btn btn-default btn-sm" %>
			  	</div>
		  	</div>
		</div>
	</div>
