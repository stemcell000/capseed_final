<%= render :partial => 'components/process_pills' %>
	<div class="panel panel-primary">
			<div class="panel-heading"> Cloning: <%= @assay.name %></div>
			<div class="panel-body">
				
					<div class="row">
						<div class="col-lg-12">
							<span class="font-success">This cloning is completed!</span>
						</div>
					</div>
					<div class="row">
						<div id= "lock_container" class="col-lg-12">
							<%= render :partial => 'assays/assay_components/complete_lock' %>			
						</div>
					</div>
				
			<div class="panel-footer row">
				    <div class="btn-group col-md-5" role="group" aria-label="...">
				    	<%= link_to 'Previous', close_assay_path(@assay.id), :class => "btn btn-default btn-sm" %>
				  	</div>
			</div>
		</div>
	</div>
</div>
